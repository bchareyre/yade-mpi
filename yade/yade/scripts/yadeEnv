setenv YADEPATH $1
setenv YADEBINPATH $YADEPATH/bin
setenv YADECOMPILATIONPATH $YADEBINPATH/compilation
setenv YADEDYNLIBPATH $YADEBINPATH/dynlib/linux
setenv YADEINCLUDEPATH $YADEPATH/yade/include
setenv LD_LIBRARY_PATH $YADEDYNLIBPATH/yade-libs:$LD_LIBRARY_PATH
setenv PATH $YADEPATH/yade/scripts/:$PATH

export YADEPATH="$1"
export YADEBINPATH="$YADEPATH/bin"
export YADECOMPILATIONPATH="$YADEBINPATH/compilation"
export YADEDYNLIBPATH="$YADEBINPATH/dynlib/linux"
export LD_LIBRARY_PATH="$LD_LIBRARY_PATH:$YADEDYNLIBPATH/yade-libs:$YADEDYNLIBPATH"
export PATH="$PATH:$YADEPATH/yade/scripts/"
export YADEINCLUDEPATH="$YADEPATH/yade/include"
